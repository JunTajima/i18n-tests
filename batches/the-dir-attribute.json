tests = []
tests[0]=["the-dir-attribute-001.html","The dir attribute: dir=rtl basic test","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If the element's dir attribute is in the rtl state, the directionality of the element is rtl."] 

tests[1]=["the-dir-attribute-002.html","The dir attribute: dir=ltr basic test","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If the element's dir attribute is in the ltr state, the directionality of the element is ltr."] 

tests[2]=["the-dir-attribute-003.html","The dir attribute: ltr context, rtl table","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"[Exploratory] When dir='rtl' is added to a table in a ltr context, (a) directional runs in a table are ordered right-to-left, (b) columns run right-to-left, (c) text is right-aligned within cells, and (d) the table is left-aligned on the page."] 

tests[3]=["the-dir-attribute-004.html","The dir attribute: rtl context, ltr table","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"[Exploratory] When dir='ltr' is added to a table in a rtl context, (a) directional runs in the table are ordered left-to-right, (b) columns run left-to-right, (c) text is left-aligned within cells, and (d) the table is right-aligned on the page."] 

tests[4]=["the-dir-attribute-005.html","The dir attribute: ordered and unordered lists","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"[Exploratory] In a rtl context, all list items should start from the right, regardless of the direction of the script in the list item."] 

tests[5]=["the-dir-attribute-006.html","The dir attribute: dl lists","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"[Exploratory] In a rtl context, all list items should start from the right, regardless of the direction of the script in the list item."] 

tests[6]=["the-dir-attribute-007.html","The dir attribute: inheritance","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If an element has no dir attribute, but has a parent element, the directionality of the element is the same as the parent element's directionality."] 

tests[7]=["the-dir-attribute-008.html","The dir attribute: invalid value and inheritance","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If an element has a dir attribute with an invalid value ('foo' or 'bar'), and has a parent element, the directionality of the element is the same as the parent element's directionality."] 

tests[8]=["the-dir-attribute-009.html","The dir attribute: invalid values left and right","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If an element has a dir attribute with an invalid value ('left' or 'right' or 'rl' or 'lr'), and has a parent element, the directionality of the element is the same as the parent element's directionality."] 

tests[9]=["the-dir-attribute-010.html","The dir attribute: default direction, basic test","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If the root element has no dir attribute, the directionality of an element is 'ltr'."] 

tests[10]=["the-dir-attribute-011.html","The dir attribute: default direction, invalid value 'right'","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If the root element has an invalid dir attribute ('right'), the directionality of an element is 'ltr'."] 

tests[11]=["the-dir-attribute-012.html","The dir attribute: default direction, invalid value 'rl'","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"If the root element has an invalid dir attribute ('rl'), the directionality of an element is 'ltr'."] 

tests[12]=["the-dir-attribute-046.html","The dir attribute: dir=auto, inline auto direction","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"When dir='auto', the direction is set according to the first strong character of the text. dir=auto is applied to an inline element here, in various base direction contexts."] 

tests[13]=["the-dir-attribute-047.html","The dir attribute: dir=auto, inline isolation","",'http://www.w3.org/TR/html5/dom.html#the-dir-attribute',"dir='auto' on an inline element will directionally isolate its contents from a following number."] 

